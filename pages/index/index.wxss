page{
  width: 100%;
  height: 100%;
  position: absolute;
  overflow: hidden;
}
.tips{
  padding: 10rpx 0;
  width: 100%;
  overflow: hidden;
  background-color: #1989fa;
  text-align: center;
  line-height: 40rpx;
}
.t1{
  color: #fff;
  font-size: 26rpx;
}
.t2{
  color: #e5e5e5;
  font-size: 24rpx;
}
.map{
  width: 100%;
  height: 600rpx;
  position: relative;
}
.btn{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  z-index: 10;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  bottom: 20rpx;
  left: 20rpx;
  border-radius: 10rpx;
}
.btn image{
  width: 30rpx;
  height: 30rpx;
  float: left;
}
.scroll{
  width: 100%;
  height: calc(100% - 700rpx);
}
.item{
  width: 100%;
  /* height: 120rpx; */
  border-bottom: 2rpx solid #f2f2f2;
}
.itemC{
  width: 90%;
  /* height: 120rpx; */
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.icon{
  transform: scale(0.7);
}

.left{
  max-width: calc(100% - 180rpx);
}
.d{
  margin-top: 15rpx;
  color: #333;
}
.dd{
  font-size: 24rpx;
  color: #999;
  line-height: 1;
  /* margin: 10rpx 0; */
  margin-bottom: 15rpx;
}


.right{
  max-width: 180rpx;
  display: flex;
  align-items: center;
}
.dis{
  font-size: 26rpx;
  color: #999;
  margin-right: 10rpx;
}
.none{
  width: 100%;
  background-color: #f2f2f2;
  color: #999;
  font-size: 26rpx;
  line-height: 50rpx;
  margin: 10rpx 0;
  text-align: center;
}
.wu{
  width: 90%;
  padding: 20rpx;
  border: 2rpx solid #f2f2f2;
  margin: 30rpx auto;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #666;
}